#------------------------------------------------------------
#        Script MySQL.
#------------------------------------------------------------


#------------------------------------------------------------
# Table: periode
#------------------------------------------------------------

CREATE TABLE periode(
        idEra   Int  Auto_increment  NOT NULL ,
        nameEra Varchar (50) NOT NULL
	,CONSTRAINT periode_PK PRIMARY KEY (idEra)
)ENGINE=InnoDB;


#------------------------------------------------------------
# Table: dinosaure
#------------------------------------------------------------

CREATE TABLE dinosaure(
        id    Int  Auto_increment  NOT NULL ,
        name  Varchar (50) NOT NULL ,
        diet  Varchar (50) NOT NULL ,
        idEra Int NOT NULL
	,CONSTRAINT dinosaure_PK PRIMARY KEY (id)

	,CONSTRAINT dinosaure_periode_FK FOREIGN KEY (idEra) REFERENCES periode(idEra)
)ENGINE=InnoDB;

